syntax = v1.0;

namespace "Omnius.Lxna.Service";

enum ThumbnailResizeType : uint8 {
    Pad = 0,
    Crop = 1,
}

enum ThumbnailFormatType : uint8 {
    Png = 0,
}

table ThumbnailMetadata {
    FileLength: uint64 = 0,
    FileLastWriteTime: timestamp = 1,
}

table ThumbnailContent {
    [Recyclable]
    Image: bytes(capacity: 33554432) = 0,
}
