syntax = v1.0;

using "Omnius.Lxna.Service";

namespace "Omnius.Lxna.Service.Internal";
option csharp_access_level = "internal";

table ThumbnailMetadata {
    FileLength: uint64 = 0,
    FileLastWriteTime: timestamp = 1,
}

table ThumbnailEntity {
    Metadata: ThumbnailMetadata = 0,
    Contents: vector<ThumbnailContent>(capacity: 8192) = 1,
}
