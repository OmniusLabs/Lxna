syntax = v1;

option csharp_namespace = "Lxna.Messages";

message LxnaOptions {
    ConfigDirectoryPath: string(1024) = 0,
}

enum ContentType : uint8 {
    Directory = 0,
    Archive = 1,
    File = 2,
}

message ContentId {
    Type: ContentType = 0,
    Path: string(1024) = 1,
}

enum ThumbnailResizeType : uint8 {
    Pad = 0,
    Crop = 1,
}

enum ThumbnailFormatType : uint8 {
    Png = 0,
}

message Thumbnail {
    [Recyclable]
    Value: memory(33554432) = 0,
}
