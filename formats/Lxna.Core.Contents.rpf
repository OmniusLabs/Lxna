syntax = v1;

using "../refs/omnix/formats/Omnix.Network.rpf";
using "Lxna.Messages.rpf";

option csharp_namespace = "Lxna.Core.Contents";
option csharp_access_level = "internal";

message FileId {
    Address: OmniAddress = 0,
    Length: uint64 = 1,
    LastWriteTime: timestamp = 2,
}

message ThumbnailsCache {
    Thumbnails: vector<LxnaThumbnail>(1024) = 0,
}
